extends layout

block content

  .header_container
    h1 Flip the Senate
  .topnav
    button#homeButton.btn.btn-outline-primary(type='button') Home
    button#amyButton.btn.btn-outline-primary(type='button') Amy
    button#dougButton.btn.btn-outline-primary(type='button') Doug
    button#markButton.btn.btn-outline-primary(type='button') Mark
    button#johnButton.btn.btn-outline-primary(type='button') John
    button#zoomButton.btn.btn-outline-primary(type='button') ZoomTime
    button#mapButton.btn.btn-outline-primary(type='button') Map
    script(src='javascripts/client.js')
  .main
    .card.text-center.text-white.bg-dark.mb-3(style='margin: 40px;')
      .card-header
        h5.card-title Zoom Meeting
      .card-body
        img(src='images/AmyM.jpg', alt='Image', width='300', height='300')
        img(src='images/DougJ.jpg', alt='Image', width='300', height='300')
        img(src='images/MarkK.jpg', alt='Image', width='300', height='300')
        img(src='images/JohnH.jpg', alt='Image', width='300', height='300')
      .card-footer
        button.btn.btn-primary(type='button') Audio
        button.btn.btn-danger(type='button') Leave Meeting
        button.btn.btn-primary(type='button') Video



   

